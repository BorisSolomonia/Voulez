{
  "name": "wolt-sync-system",
  "version": "2.0.0",
  "description": "High-performance inventory synchronization service for Fina to Wolt with Hybrid Sync (Solution 5): Bootstrap + Priority + Adaptive + Background Worker",
  "main": "dist/index.js",
  "bin": {
    "wolt-cli": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "dev": "ts-node src/index.ts",
    "cli": "node dist/cli/index.js",
    "cli:dev": "ts-node src/cli/index.ts",
    "bootstrap:all": "node dist/cli/index.js bootstrap --all",
    "pm2:start": "pm2 start ecosystem.config.js --only wolt-sync-all",
    "pm2:stop": "pm2 stop wolt-sync-all",
    "pm2:restart": "pm2 restart wolt-sync-all",
    "pm2:logs": "pm2 logs wolt-sync-all",
    "test": "vitest run",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "wolt",
    "fina",
    "sync",
    "inventory"
  ],
  "author": "Voulez-Vous",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "p-queue": "^7.4.1",
    "winston": "^3.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.0",
    "ts-node": "^1.7.1",
    "typescript": "^5.1.6",
    "vitest": "^4.0.17"
  }
}
